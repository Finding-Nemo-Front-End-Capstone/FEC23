{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "jest"
    ],
    "rules": {
        "max-len": "off",
        "no-unused-vars": "off",
        "camelcase": "off",
        "no-plusplus": "off",
        "react/prop-types": 0,
        "import/extensions": 0
    }
}
